    "## Nulos, valores unicos y duplicados"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "43b49dc2-fa72-4daf-a793-ca324e503b61",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Genera un DataFrame con el número de nulos, el porcentaje de nulos y valores unicos por columna.\n",
    "# Tambien añadimos si hay filas duplicadas\n",
    "# Llamada a la función y ordenar por el porcentaje de nulos\n",
    "# nulos_df = nulos(df, ordenar_por='Porcentaje Nulos')\n",
    "\n",
    "# Llamada sin impresión\n",
    "# nulos_df = nulos(df, imprimir=False)\n",
    "\n",
    "def nulos(df, ordenar_por='Nulos', imprimir=True):\n",
    "    # Calcular el número de nulos, porcentaje de nulos y valores únicos por columna\n",
    "    nulos_por_columna = df.isnull().sum()\n",
    "    porcentaje_nulos_por_columna = (nulos_por_columna / len(df)) * 100\n",
    "    valores_unicos = df.nunique()\n",
    "    \n",
    "    # Calcular el número de filas duplicadas\n",
    "    filas_duplicadas = df.duplicated().sum()\n",
    "    \n",
    "    # Crear un DataFrame con los resultados por columna\n",
    "    nulos_df = pd.DataFrame({\n",
    "        'Nulos': nulos_por_columna, \n",
    "        'Porcentaje Nulos': porcentaje_nulos_por_columna.round(2),  # Redondear porcentaje a 2 decimales\n",
    "        'Valores únicos': valores_unicos\n",
    "    })\n",
    "    \n",
    "    # Añadir una fila especial para las filas duplicadas\n",
    "    nulos_df.loc['Duplicados'] = [filas_duplicadas, (filas_duplicadas / len(df)) * 100, 'N/A']\n",
    "    \n",
    "    # Ordenar el DataFrame por el criterio seleccionado, ignorando la fila \"Duplicados\" si se quiere\n",
    "    if ordenar_por in nulos_df.columns:\n",
    "        nulos_df = nulos_df.sort_values(by=ordenar_por, ascending=False)\n",
    "    \n",
    "    # Imprimir los resultados si se solicita\n",
    "    if imprimir:\n",
    "        print(\"Número de nulos, porcentaje de nulos y valores únicos por columna, incluyendo duplicados:\")\n",
    "        print(nulos_df)\n",
    "    \n",
    "    return nulos_df\n"
   ]
