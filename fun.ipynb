    "            print(f\"\\n{'Columna:':<12} {col}  -  Tipo de datos: {df[col].dtype}\")\n",
    "            print(f\"{'Valores nulos:':<12} {df[col].isnull().sum()}  -  Valores distintos: {df[col].nunique()}\")\n",
    "            \n",
    "            # Mostrar los valores más frecuentes, hasta 10 valores\n",
    "            print(f\"{'Valores más frecuentes:':<12}\")\n",
    "            top_values = df[col].value_counts().head(10)\n",
    "            for valor, cuenta in top_values.items():\n",
    "                print(f\"{str(valor):<20} {cuenta}\")\n",
