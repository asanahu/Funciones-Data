    "# Cargar archivo\n",
    "\n",
    "def cargar_archivo(nombre_archivo):\n",
    "    try:\n",
    "        # Obtener la ruta completa del archivo\n",
    "        ruta_archivo = os.path.abspath(nombre_archivo)\n",
    "        extension = os.path.splitext(nombre_archivo)[1].lower()\n",
    "        \n",
    "        # Determinar el tipo de archivo y cargarlo\n",
    "        if extension == '.csv':\n",
    "            df = pd.read_csv(ruta_archivo)\n",
    "        elif extension == '.json':\n",
    "            df = pd.read_json(ruta_archivo)\n",
    "        elif extension in ['.xls', '.xlsx']:\n",
    "            df = pd.read_excel(ruta_archivo)\n",
    "        elif extension == '.parquet':\n",
    "            df = pd.read_parquet(ruta_archivo)\n",
    "        elif extension == '.h5':\n",
    "            df = pd.read_hdf(ruta_archivo)\n",
    "        elif extension == '.feather':\n",
    "            df = pd.read_feather(ruta_archivo)\n",
    "        elif extension == '.pkl':\n",
    "            df = pd.read_pickle(ruta_archivo)\n",
    "        elif extension == '.dta':\n",
    "            df = pd.read_stata(ruta_archivo)\n",
    "        elif extension == '.sav':\n",
    "            df = pd.read_spss(ruta_archivo)\n",
    "        else:\n",
    "            raise ValueError(f\"Formato de archivo no soportado: {extension}\")\n",
    "        \n",
    "        # Imprimir el nombre del archivo\n",
    "        print(f\"Archivo cargado: {os.path.basename(ruta_archivo)}\")\n",
    "        \n",
    "        # Imprimir el head del DataFrame\n",
    "        print(\"\\nPrimeras filas del dataset:\")\n",
    "        print(df.head())\n",
    "        \n",
    "        # Resumen estadístico\n",
    "        print(\"\\nResumen estadístico del DataFrame:\")\n",
    "        print(df.describe())\n",
    "        \n",
    "        return df\n",
